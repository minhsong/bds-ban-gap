@model BDSBanGap.Models.DBContext.District

<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>

@using (Html.BeginForm("","",FormMethod.Post,new {id="Form"})) {
    @Html.ValidationSummary(true)
    <fieldset>
        <table>

                @Html.HiddenFor(model => model.DistrictID)
        <tr>
            <td>Tên quận
            </td>
            <td>@Html.EditorFor(model => model.DistrictName)
            @Html.ValidationMessageFor(model => model.DistrictName)
            </td>
        </tr>

        <tr>
            <td>Ghi chú
            </td>
            <td>@Html.EditorFor(model => model.Description)
            @Html.ValidationMessageFor(model => model.Description)
            </td>
        </tr>
    </table>
        
    </fieldset>
}
