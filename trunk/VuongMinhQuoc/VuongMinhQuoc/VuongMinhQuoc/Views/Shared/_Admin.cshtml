                                            <tr>
                                                <th valign="top">Description:</th>
                                                <td>
                                                    <textarea rows="" cols="" class="form-textarea"></textarea></td>
                                                <td></td>
                                            </tr>
                                            <tr>
                                                <th>Image 1:</th>
                                                <td>
                                                    <input type="file" class="file_1" /></td>
                                                <td>
                                                    <div class="bubble-left"></div>
                                                    <div class="bubble-inner">JPEG, GIF 5MB max per image</div>
