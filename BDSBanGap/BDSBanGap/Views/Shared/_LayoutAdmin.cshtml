<script type="text/javascript">
    MenuOpen('@HttpContext.Current.Request.RequestContext.RouteData.Values["controller"].ToString().Trim()');
    function MenuOpen(Controller) {
        $('.'+Controller).attr("class","open")
    }
</script>

