<script type="text/javascript">
    MenuOpen('@HttpContext.Current.Request.RequestContext.RouteData.Values["controller"].ToString().Trim()');
    function MenuOpen(Controller) {
        $('.'+Controller).attr("class","open")
    }
    $('.dropdown').click(function () {
        var menu = $(this).children('.dropdown-menu');
        if (menu.css('display') == 'none') {
            menu.css('display', 'block');
        } else {
            menu.css('display', 'none');
        }
    });
</script>

